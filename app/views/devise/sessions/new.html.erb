<center>

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
<h2>Log in</h2>
  <div class="field">
    <%= f.label :email %><br />
    <%= f.email_field :email, autofocus: true %>
  </div>

  <div class="field">
    <%= f.label :password %><br />
    <%= f.password_field :password, autocomplete: "off", :class => "test" %>
  </div>

  <% if devise_mapping.rememberable? -%>
    <div class="field">
    <%= f.check_box :remember_me %>
    <%= f.label   :remember_me %>
      
    </div>
  <% end -%>

  <div class="actions">
    <%= f.submit "Log in" %><br>
   <a href="http://localhost:3000/users/sign_up">sign up</a></div>
   
   <a href="http://localhost:3000/users/password/new">forgot password?</a>

  </div>
<% end %>

<%= render "devise/shared/links" %>
</center>
